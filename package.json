{
    "name": "vscode-snakemake",
    "displayName": "Snakemake Language",
    "description": "Basic language, syntax, and snippet support for Snakemake files",
    "version": "0.0.3",
    "publisher": "alping",
    "repository": "https://gitlab.com/alping/vscode-snakemake",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "snakemake",
            "aliases": ["Snakemake", "snakemake"],
            "filenames": ["Snakefile"],
            "extensions": [".smk"],
            "configuration": "./languages/snakemake.json"
        }],
        "grammars": [{
            "language": "snakemake",
            "scopeName": "source.python.snakemake",
            "path": "./syntaxes/snakemake.tmLanguage.json"
        }],
        "snippets": [{
              "language": "snakemake",
              "path": "./snippets/snakemake.json"
        }]
    }
}
